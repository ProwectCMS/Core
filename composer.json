{
    "name": "prowectcms/core",
    "description": "Powerful framework and CMS for rapid development based on Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Alina Kessler",
            "email": "alina.kessler@prowect.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ProwectCMS\\Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "ProwectCMS\\Core\\Tests\\": "tests",
          "ProwectCMS\\Core\\Database\\Seeders\\Test\\": "database/seeders/test/"
        }
    },
    "require": {
        "illuminate/support": "^8.64",
        "spatie/laravel-settings": "^2.1",
        "spatie/laravel-event-sourcing": "5.*",
        "laravel/sanctum": "^2.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.21",
        "sempro/phpunit-pretty-print": "^1.4"
    },
    "extra": {
      "laravel": {
          "providers": [
              "ProwectCMS\\Core\\Providers\\ServiceProvider",
              "ProwectCMS\\Core\\Providers\\AuthServiceProvider",
              "ProwectCMS\\Core\\Providers\\EventSourcingServiceProvider",
              "ProwectCMS\\Core\\Providers\\SanctumServiceProvider"
          ]
      }
  }
}
